{
  "schema_version": 1,
  "quilt_loader": {
    "group": "${group}",
    "id": "storagecabinet",
    "version": "${version}",
    "intermediate_mappings": "net.fabricmc:intermediary",
    "entrypoints": {
      "init": [
        "com.ashindigo.storagecabinet.quilt.StorageCabinetQuilt"
      ],
      "client_init": [
        "com.ashindigo.storagecabinet.quilt.StorageCabinetQuiltClient"
      ]
    },
    "depends": [
      {
        "id": "quilt_loader",
        "versions": ">=0.17.0-"
      },
      {
        "id": "minecraft",
        "version": ">=1.19"
      },
      {
        "id": "architectury",
        "version": ">=5.7.28"
      }
    ],
    "metadata": {
      "name": "Storage Cabinet",
      "description": "Adds storage cabinets to Minecraft",
      "contributors": {
        "AshIndigo": "Author",
        "MoonlitLamps": "Textures",
        "LotuxPunk": "French Localization"
      },
      "contact": {
        "homepage": "https://www.curseforge.com/minecraft/mc-mods/storage-cabinet",
        "sources": "https://github.com/AshIndigo/Filing_Cabinet",
        "issues": "https://github.com/AshIndigo/Storage_Cabinet/issues"
      },
      "license": "MIT",
      "icon": "icon.png"
    }
  },
  "access_widener": "storagecabinet.accesswidener",
  "minecraft": {
    "environment": "*"
  }
}